{
    "name": "topology-builder-frontend",
    "version": "1.0.0",
    "description": "Topology Builder Frontend",
    "scripts": {
        "prebuild": "node build-template.js",
        "build": "npm run prebuild && parcel build src/index.html --dist-dir dist --public-url /static",
        "predev": "node build-template.js",
        "dev": "npm run predev && parcel src/index.html --open",
        "watch": "node build-template.js && parcel watch src/index.html --dist-dir dist --public-url /static",
        "render": "node build-template.js"
    },
    "dependencies": {
        "bootstrap": "^5.3.2",
        "bootstrap-icons": "^1.11.1"
    },
    "devDependencies": {
        "@parcel/transformer-sass": "^2.16.0",
        "nunjucks": "^3.2.4",
        "parcel": "^2.10.0",
        "parcel-reporter-static-files-copy": "^1.5.3",
        "sass": "^1.69.0"
    }
}
